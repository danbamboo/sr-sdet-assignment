{
  "consumer": {
    "name": "dashboard_api"
  },
  "provider": {
    "name": "calendar_service"
  },
  "interactions": [
    {
      "description": "A request for an calendar event",
      "providerState": "Calendar event/s do not exist for user ID",
      "request": {
        "method": "get",
        "path": "/events",
        "query": "invalid_user_id_params=0"
      },
      "response": {
        "status": 404,
        "headers": {
          "Content-Type": "text/html; charset=utf-8"
        },
        "body": "User Does Not Exist"
      }
    },
    {
      "description": "A request for an calendar event",
      "providerState": "Calendar event/s exist for user ID",
      "request": {
        "method": "get",
        "path": "/events",
        "query": "user_id=1"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": {
          "events": [
            {
              "id": 1,
              "name": "Hangout",
